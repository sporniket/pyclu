{
  "cli":{
    "metadata":{
      "command":"ap",
      "name":"Assemble Project, by Sporniket",
      "version":"0.0.0-SNAPSHOT"
    },
    "env":{
      "variables":{
        "CLI_DATA":"${CLI_DIR}/data",
        "CLI_DIR":"${BASEDIR}/zz_cli",
        "TOOLS_DIR":"${BASEDIR}/zz_tools"
      },
      "pathes":[
        "${TOOLS_DIR}/bin"
      ]
    }
  },
  "commands":[
    {
      "name":"install",
      "help":"Install vasm, Hatari,..."
    },
    {
      "name":"setup",
      "help":"Setup utility."
    },
    {
      "name":"scandeps",
      "help":"Scan source files into makefile dependencies variables."
    },
    {
      "name":"build",
      "help":"Build utility."
    },
    {
      "name":"run",
      "help":"Run utility."
    }
  ]
}